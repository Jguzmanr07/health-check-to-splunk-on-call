{
  "name": "health-check-to-splunk-on-call",
  "private": true,
  "version": "0.0.0",
  "description": "Relay Cloudflare health check payload to Splunk On-Call (FKA Victorops)",
  "keywords": [
    "Cloudflare",
    "Health-Check",
    "Splunk-On-Call",
    "Victorops",
    "Notification",
    "Incident"
  ],
  "author": "Fiverr",
  "license": "Unlicense",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "esbuild src/index.ts --outfile=index.js --format=esm  --bundle",
    "format": "prettier -w .",
    "lint": "prettier -c .",
    "pretest": "npm run build",
    "publish": "wrangler publish",
    "start": "wrangler dev src/index.ts",
    "test": "node --test tests/index.js",
    "wrangler": "wrangler"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.9.0",
    "esbuild": "^0.14.38",
    "prettier": "^2.6.2",
    "wrangler": "^2.0.3"
  }
}
